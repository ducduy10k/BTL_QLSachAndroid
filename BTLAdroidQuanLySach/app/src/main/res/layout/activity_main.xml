<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/btnsachmanage"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/bookmanage"
        android:src="@drawable/button_advance"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@drawable/button_advance" />

    <ImageButton
        android:id="@+id/btnbillmanage"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/bill100"
        android:src="@drawable/button_advance"
        app:layout_constraintStart_toStartOf="@+id/btnsachmanage"
        app:layout_constraintTop_toBottomOf="@+id/lbbook"
        tools:src="@drawable/button_advance" />

    <ImageButton
        android:id="@+id/btnusermanage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/user"
        android:src="@drawable/button_advance"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.574"
        app:layout_constraintStart_toEndOf="@+id/btnsachmanage"
        app:layout_constraintTop_toTopOf="@+id/btnsachmanage"
        tools:src="@drawable/user" />

    <ImageButton
        android:id="@+id/btnstatistical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/statistics100"
        android:src="@drawable/button_advance"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btnusermanage"
        app:layout_constraintTop_toTopOf="@+id/btnbillmanage" />

    <TextView
        android:id="@+id/lbuser"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="User"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@+id/btnusermanage"
        app:layout_constraintTop_toBottomOf="@+id/btnusermanage" />

    <TextView
        android:id="@+id/lbbook"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Bookmanage"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@+id/btnsachmanage"
        app:layout_constraintTop_toBottomOf="@+id/btnsachmanage" />

    <TextView
        android:id="@+id/lbbook2"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Bill"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@+id/btnsachmanage"
        app:layout_constraintTop_toBottomOf="@+id/btnbillmanage" />

    <ImageButton
        android:id="@+id/btnsettings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:background="@drawable/button_advance"
        android:src="@drawable/setting"
        app:layout_constraintStart_toStartOf="@+id/btnsachmanage"
        app:layout_constraintTop_toBottomOf="@+id/btnbillmanage"
        tools:src="@drawable/setting" />

    <TextView
        android:id="@+id/lbsettings"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Settings"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@+id/btnsachmanage"
        app:layout_constraintTop_toBottomOf="@+id/btnsettings" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="Statistical"
        app:layout_constraintStart_toStartOf="@+id/btnstatistical"
        app:layout_constraintTop_toBottomOf="@+id/btnstatistical" />
</androidx.constraintlayout.widget.ConstraintLayout>